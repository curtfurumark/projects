<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constrainLayout_music_project_adapter"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView_sessionAdapter_heading"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="heading"
        android:textStyle="bold"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_music_project_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_sessionAdapter_heading" />

    <TextView
        android:id="@+id/textView_updated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/textView_music_project_id"
        app:layout_constraintTop_toBottomOf="@+id/textView_sessionAdapter_heading" />
</androidx.constraintlayout.widget.ConstraintLayout>